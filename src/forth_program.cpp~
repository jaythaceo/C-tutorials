#include <iostream>
using namespace std;

#define len 100
int main(int argc, const char * argv[])
{
    
  double array[len];
  double ave;
  double sum = 0;

  for (int i = 0; i < len; i++) {
        
    sum += array[i];
    ave = sum / len;
        
  }
    
  cout << "Average: " << ave << endl;
    
    
  return 0;
}

